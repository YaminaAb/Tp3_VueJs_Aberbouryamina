{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport ProductDataService from '@/services/ProductDataService';\nexport default {\n  props: ['removeInv', 'inventory', 'remove', 'updateInv'],\n  data() {\n    return {\n      submitted: false,\n      product: {},\n      id: parseInt(this.$route.params.id)\n    };\n  },\n  methods: {\n    updateProduct() {\n      ProductDataService.update(this.id, this.product).then(response => {\n        this.updateInv(this.productIndex, this.product);\n        this.submitted = true;\n      });\n    },\n    deleteProduct() {\n      ProductDataService.delete(this.id).then(response => {\n        this.removeInv(this.productIndex);\n        this.remove(this.product.name);\n        this.$router.push({\n          name: 'home'\n        });\n      });\n    }\n  },\n  computed: {\n    productIndex() {\n      const index = this.inventory.findIndex(p => {\n        return p.id === this.id;\n      });\n      return index;\n    }\n  },\n  mounted() {\n    ProductDataService.get(this.id).then(response => {\n      this.product = response.data;\n    });\n  }\n};","map":{"version":3,"mappings":";AA2EA,OAAOA,kBAAiB,MAAO,+BAA8B;AAE7D,eAAe;EACXC,KAAK,EAAE,CAAC,WAAW,EAAE,WAAW,EAAE,QAAQ,EAAE,WAAW,CAAC;EACxDC,IAAI,GAAG;IACH,OAAO;MACHC,SAAS,EAAE,KAAK;MAChBC,OAAO,EAAE,CAAC,CAAC;MACXC,EAAE,EAAEC,QAAQ,CAAC,IAAI,CAACC,MAAM,CAACC,MAAM,CAACH,EAAE;IACtC;EACJ,CAAC;EACDI,OAAO,EAAE;IACLC,aAAa,GAAG;MACZV,kBAAkB,CAACW,MAAM,CAAC,IAAI,CAACN,EAAE,EAAE,IAAI,CAACD,OAAO,EAC1CQ,IAAI,CAACC,QAAO,IAAK;QACd,IAAI,CAACC,SAAS,CAAC,IAAI,CAACC,YAAY,EAAE,IAAI,CAACX,OAAO;QAC9C,IAAI,CAACD,SAAQ,GAAI,IAAG;MACxB,CAAC;IACT,CAAC;IACDa,aAAa,GAAG;MACZhB,kBAAkB,CAACiB,MAAM,CAAC,IAAI,CAACZ,EAAE,EAC5BO,IAAI,CAACC,QAAO,IAAK;QACd,IAAI,CAACK,SAAS,CAAC,IAAI,CAACH,YAAY;QAChC,IAAI,CAACI,MAAM,CAAC,IAAI,CAACf,OAAO,CAACgB,IAAI;QAC7B,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;UAAEF,IAAI,EAAE;QAAO,CAAC;MACtC,CAAC;IACT;EACJ,CAAC;EACDG,QAAQ,EAAE;IACNR,YAAY,GAAG;MACX,MAAMS,KAAI,GAAI,IAAI,CAACC,SAAS,CAACC,S