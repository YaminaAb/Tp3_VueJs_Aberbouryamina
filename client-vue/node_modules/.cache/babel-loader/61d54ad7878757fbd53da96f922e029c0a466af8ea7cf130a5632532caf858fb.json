{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport ProductDataService from '@/services/ProductDataService';\nexport default {\n  props: ['inventory', 'updateInv'],\n  data() {\n    return {\n      product: {},\n      id: parseInt(this.$route.params.id)\n    };\n  },\n  methods: {\n    updateProduct() {\n      ProductDataService.update(this.id, this.product).then(() => {\n        this.updateInv(this.productIndex, this.product);\n        this.$router.push({\n          name: 'home'\n        });\n      });\n    }\n  },\n  computed: {\n    productIndex() {\n      const index = this.inventory.findIndex(p => {\n        return p.id === this.id;\n      });\n      return index;\n    }\n  },\n  mounted() {\n    ProductDataService.get(this.id).then(response => {\n      this.product = response.data;\n    });\n  }\n};","map":{"version":3,"mappings":";AA2DA,OAAOA,kBAAiB,MAAO,+BAA8B;AAE7D,eAAe;EACXC,KAAK,EAAE,CAAC,WAAW,EAAE,WAAW,CAAC;EACjCC,IAAI,GAAG;IACH,OAAO;MACHC,OAAO,EAAE,CAAC,CAAC;MACXC,EAAE,EAAEC,QAAQ,CAAC,IAAI,CAACC,MAAM,CAACC,MAAM,CAACH,EAAE;IACtC;EACJ,CAAC;EACDI,OAAO,EAAE;IACLC,aAAa,GAAG;MACZT,kBAAkB,CAACU,MAAM,CAAC,IAAI,CAACN,EAAE,EAAE,IAAI,CAACD,OAAO,EAC1CQ,IAAI,CAAC,MAAM;QACR,IAAI,CAACC,SAAS,CAAC,IAAI,CAACC,YAAY,EAAE,IAAI,CAACV,OAAO;QAC9C,IAAI,CAACW,OAAO,CAACC,IAAI,CAAC;UAAEC,IAAI,EAAE;QAAO,CAAC;MACtC,CAAC;IACT;EACJ,CAAC;EACDC,QAAQ,EAAE;IACNJ,YAAY,GAAG;MACX,MAAMK,KAAI,GAAI,IAAI,CAACC,SAAS,CAACC,SAAS,CAAEC,CAAC,IAAK;QAC1C,OAAOA,CAAC,CAACjB,EAAC,KAAM,IAAI,CAACA,EAAC;MAC1B,CAAC;MACD,OAAOc,KAAI;IACf;EACJ,CAAC;EACDI,OAAO,GAAG;IACNtB,kBAAkB,CAACuB,GAAG,CAAC,IAAI,CAACnB,EAAE,EACzBO,IAAI,CAACa,QAAO,IAAK;MACd,IAAI,CAACrB,OAAM,GAAIqB,QAAQ,CAACtB,IAAG;IAC/B,CAAC;EACT;AACJ","names":["ProductDataService","props","data","product","id","parseInt","$route","params","methods","updateProduct","update","then","updateInv","productIndex","$router","push","name","computed","index","inventory","findIndex","p","mounted","get","response"],"sourceRoot":"","sources":["C:\\wa