         <input type=\"text\" class=\"form-control\" id=\"productPrice\" placeholder=\"\" v-model.number='product.price' required />\r\n                                            <div class=\"invalid-feedback\">Price is required.</div>\r\n                                        </div>\r\n                                    </div>\r\n                                    <div class=\"col-12\">\r\n                                        <label for=\"productDescription\" class=\"form-label\">Product Description</label>\r\n                                        <textarea class=\"form-control\" id=\"productDescription\"  v-model=\"product.description\" placeholder=\"\"></textarea>\r\n                                        <div class=\"invalid-feedback\">Valid description</div>\r\n                                    </div>\r\n                                    <div class=\"col-12\">\r\n                                        <label for=\"productType\" class=\"form-label\">Product Type</label>\r\n                                        <select class=\"form-control\" id=\"productType\" placeholder=\"\" v-model='product.type' required>\r\n                                            <option value=\"\">Select</option>\r\n                                            <option value=\"Eletronic\">Eletronic</option>\r\n                                            <option value=\"Game\">Game</option>\r\n                                            <option value=\"House\">House</option>\r\n                                            <option value=\"Mobile\">Mobile</option>\r\n                                        </select>\r\n                                        <div class=\"invalid-feedback\"> Valid photo path is required.</div>\r\n                                    </div>\r\n                                    <button class=\"w-100 btn btn-success btn-lg mt-3\" type=\"button\" @click=\"updateProduct\">Update </button>\r\n                                </div>\r\n                                \r\n                                <hr class=\"my-4\">\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </section>\r\n    </div>\r\n</template>\r\n   \r\n<script>\r\nimport ProductDataService from '@/services/ProductDataService'\r\n\r\nexport default {\r\n    props: ['inventory', 'updateInv'],\r\n    data() {\r\n        return {\r\n            product: {},\r\n            id: parseInt(this.$route.params.id)\r\n        }\r\n    },\r\n    methods: {\r\n        updateProduct() {\r\n            ProductDataService.update(this.id, this.product)\r\n                .then(() => {\r\n                    this.updateInv(this.productIndex, this.product)\r\n                    this.$router.push({ name: 'home' })\r\n                })\r\n        }\r\n    },\r\n    computed: {\r\n        productIndex() {\r\n            const index = this.inventory.findIndex((p) => {\r\n                return p.id === this.id\r\n            })\r\n            return index\r\n        }\r\n    },\r\n    mounted() {\r\n        ProductDataService.get(this.id)\r\n            .then(response => {\r\n                this.product = response.data\r\n            })\r\n    }\r\n}\r\n</script>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}